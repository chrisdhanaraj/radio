{
  "name": "project-zenith-client",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "license": "MIT",
  "scripts": {
    "fix": "eslint --fix **/*.js",
    "style": "gulp serve",
    "dev": "webpack-dev-server --config ./webpack.config.dev.js",
    "build": "webpack --config ./webpack.config.dev.js || gulp sass",
    "start": "node app.js"
  },
  "engines": {
    "node": "6.9.4"
  },
  "dependencies": {
    "@console/bluemix-components-react": "^2.1.2",
    "babel-preset-react": "^6.23.0",
    "body-parser": "^1.17.1",
    "browser-sync": "^2.18.8",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "css-loader": "^0.26.1",
    "express": "^4.15.2",
    "history": "^4.6.0",
    "jsonwebtoken": "^7.3.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "lodash.merge": "^4.6.0",
    "medium-editor": "^5.23.0",
    "mongoose": "^4.9.3",
    "morgan": "^1.8.1",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.1",
    "query-string": "^4.3.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-history": "^0.18.2",
    "react-icons": "^2.2.3",
    "react-router-dom": "^4.0.0",
    "recharts": "^0.22.0",
    "sass-loader": "^6.0.1",
    "style-loader": "^0.13.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.3",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-stage-1": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-sass": "^3.1.0",
    "ignore-loader": "^0.1.2",
    "react-hot-loader": "3.0.0-beta.6",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
      }
    },
    "plugins": [
      "react"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": "airbnb",
    "rules": {
      "arrow-parens": [
        0
      ],
      "comma-dangle": [
        0
      ],
      "no-else-return": [
        0
      ],
      "no-unused-vars": [
        "warn",
        {
          "ignoreRestSiblings": true
        }
      ],
      "class-methods-use-this": [
        0
      ],
      "no-underscore-dangle": [
        0
      ],
      "react/jsx-closing-bracket-location": [
        0
      ],
      "react/jsx-indent-props": [
        0
      ],
      "react/jsx-indent": [
        0
      ],
      "react/prefer-stateless-function": [
        0
      ],
      "react/prop-types": [
        0
      ],
      "react/jsx-filename-extension": [
        0
      ]
    }
  }
}
